    "typescript": "4.0.5",
    "@novaleaf/xlib-build-tools": "0.0.2-dev.10",
    "@novaleaf/eslint-config": "0.0.1",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "eslint": "7.13.0",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-react": "7.21.5"
  },
  "dependencies": {
    "xlib": "18.0.0-dev.10"
