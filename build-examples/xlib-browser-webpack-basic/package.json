    "@types/react-dom": "^16.9.8",
    "@types/webpack-env": "1.15.3",
    "@xlib/build-tools": "workspace:*",
    "@xlib/eslint-config": "workspace:*",
    "css-loader": "4.3.0",
    "eslint": "7.10.0",
    "html-webpack-plugin": "4.5.0",
    "null-loader": "^4.0.0",
    "source-map-loader": "^1.1.0",
    "style-loader": "~1.2.1",
    "threads-plugin": "^1.3.3",
    "typescript": "4.0.3",
    "webpack": "4.44.2"
